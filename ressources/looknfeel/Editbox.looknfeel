    <WidgetLook name="Editbox/Titlebar">
        <PropertyDefinition name="CaptionColour" initialValue="FFFFFFFF" redrawOnWrite="true" type="ColourRect"/>
        <Property name="NonClient" value="true" />
        <Property name="Size" value="{{0, 256}, {0, 32}}" />
        <ImagerySection name="main">
            <ImageryComponent>
                <Image name="Editbox/NewTitlebarLeft" />
                <VertFormat type="Stretched" />
            </ImageryComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="RightEdge"/>
                            <OperatorDim op="Add">
                                <ImageDim name="Editbox/SysAreaRight" dimension="Width"/>
                                <ImageDim name="Editbox/SysAreaMiddle" dimension="Width" />
                            </OperatorDim>
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="Editbox/NewTitlebarRight" />
                <VertFormat type="Stretched" />
                <HorzFormat type="RightAligned" />
            </ImageryComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="RightEdge"/>
                            <ImageDim name="Editbox/SysAreaRight" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="Editbox/SysAreaMiddle" />
                <VertFormat type="Stretched" />
                <HorzFormat type="RightAligned" />
            </ImageryComponent>
            <ImageryComponent>
                <Image name="Editbox/SysAreaRight" />
                <VertFormat type="Stretched" />
                <HorzFormat type="RightAligned" />
            </ImageryComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><ImageDim name="Editbox/NewTitlebarLeft" dimension="Width" /></Dim>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <OperatorDim op="Subtract">
                                <OperatorDim op="Subtract">
                                    <UnifiedDim scale="1" type="RightEdge"/>
                                    <ImageDim name="Editbox/SysAreaRight" dimension="Width"/>
                                </OperatorDim>
                                <ImageDim name="Editbox/SysAreaMiddle" dimension="Width"/>
                            </OperatorDim>
                            <ImageDim name="Editbox/NewTitlebarRight" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="Editbox/NewTitlebarMiddle" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
        <ImagerySection name="caption">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge" ><ImageDim name="Editbox/NewTitlebarLeft" dimension="Width" /></Dim>
